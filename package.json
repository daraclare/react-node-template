{
  "name": "ReactJS",
  "version": "1.0.0",
  "description": "ReactJS Project",
  "scripts": {
    "prestart": "NODE_ENV=development",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node server/main.js",
    "lint": "node_modules/.bin/esw webpack.config src tools tests --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir ./build/dist",
    "remove-dist": "node_modules/.bin/rimraf ./build/dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "NODE_ENV=production babel-node tools/build.js",
    "postbuild": "babel-node server/main.js",
    "security-check": "nsp check"
  },
  "author": "Dara Carolan",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "es6-promise": "4.1.0",
    "prop-types": "15.5.8",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router-dom": "4.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-watch": "3.0.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "2.0.0",
    "jsdom": "9.12.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "nsp": "^2.6.3",
    "open": "0.0.5",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "svg-react-loader": "0.3.7",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.0",
    "webpack-md5-hash": "0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daraclare/react-node-template.git"
  }
}
