{
  "name": "ReactJS",
  "version": "1.0.0",
  "description": "ReactJS Project",
  "scripts": {
    "prestart": "npm run security-check",
    "start":
      "cross-env NODE_ENV=development npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node build/server.js",
    "lint": "esw webpack.config.js src tools build tests --color",
    "lint:watch": "npm run lint -- --watch",
    "test":
      "cross-env NODE_ENV=test nyc mocha --reporter progress tools/testSetup.js src/**/*.spec.js tests/**/*.spec.js --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "precoverage": "npm run test",
    "coverage": "babel-node tools/open-coverage.js",
    "clean-dist": "node_modules/.bin/rimraf ./dist && mkdirp ./dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build":
      "cross-env NODE_ENV=production babel-node build/build.js && cp public/* dist/",
    "start:dist": "cross-env NODE_ENV=production babel-node build/server.js",
    "security-check": "nsp check"
  },
  "nyc": {
    "require": ["babel-register"],
    "include": ["src/**/*.js"],
    "exclude": ["src/**/*.spec.js"],
    "reporter": ["lcov", "text"],
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "author": "Dara Carolan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "es6-promise": "^4.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "eventsource-polyfill": "~0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "~0.8.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.1",
    "nsp": "^2.6.3",
    "nyc": "^11.0.2",
    "open": "~0.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "~0.16.1",
    "url-loader": "~0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-md5-hash": "~0.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daraclare/react-node-template"
  }
}
